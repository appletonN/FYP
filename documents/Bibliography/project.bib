  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1751-1097.1981.tb08993.x},
  urldate = {2021-02-27},
  abstract = {Abstract\textemdash{} Man is the only eukaryotic organism in which it has been reported that the circadian system cannot be entrained to a 24-h period by a simple light-dark (LD) cycle. In this paper, we reexamine the evidence for that claim and demonstrate that there were some fundamental flaws in the experimental design of the previous studies on which this conclusion was based. We report new studies in which we tested the efficacy of LD cycles in entraining the circadian rhythms of human subjects living in isolation from environmental time cues. We found that the cyclic alternation of light and dark, when applied to human subjects in a comparable way to experiments in other species, was an effective entraining agent. Our results and a critical review of the literature indicate that a LD cycle alone can be an effective environmental synchronizer of the human circadian timing system. Other factors, such as the knowledge of time of day, social contacts, the feeding schedule, and the imposed rest-activity schedule may contribute to stable entrainment, although their relative strengths as synchronizers have yet to be determined.},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1751-1097.1981.tb08993.x},
  file = {C\:\\Users\\Nick\\Zotero\\storage\\MM4UKE7Y\\j.1751-1097.1981.tb08993.html},
  journal = {Photochemistry and Photobiology},
  language = {en},
  number = {2}
}

