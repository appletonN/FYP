@article{czeislerBrightLightResets1986,
  title = {Bright Light Resets the Human Circadian Pacemaker Independent of the Timing of the Sleep-Wake Cycle},
  author = {Czeisler, C. A. and Allan, J. S. and Strogatz, S. H. and Ronda, J. M. and Sanchez, R. and Rios, C. D. and Freitag, W. O. and Richardson, G. S. and Kronauer, R. E.},
  year = {1986},
